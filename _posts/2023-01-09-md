{% highlight java %}

package day_03;

public class CastingCharmain {

	public static void main(String[] args) {
		char ch = 'a';
		int i = ch;  //문자 -> 정수
		System.out.println(ch+","+i);
		
		i = 100;
		ch = (char)i;
		System.out.println(ch+","+i);
		
		ch = 'a'+6;
		System.out.println(ch+","+(int)ch);
		
		ch = '가';
		System.out.println(ch+","+(int)ch);
		
		ch = '힣';
		System.out.println(ch+","+(int)ch);
		
		ch = '月';
		System.out.println(ch+","+(int)ch);
		
	}

}

