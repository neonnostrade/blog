{% highlight java %}

package day_05;

import java.util.Iterator;

public class ForStar {

	public static void main(String[] args) {
		
		for (int line = 0; line < 5; line++) {
			
			for (int star = 0; star <= line; star++) {
				System.out.print("*");
			}
			System.out.println();
			
		}
		
		System.out.println("----------------------------------------");
		
			for (int line = 0; line < 5; line++) {
			
			for (int star = 5; star > line; star--) {
				System.out.print("*");
			}
			System.out.println();
			
		}
			
			System.out.println("----------------------------------------");
			
			for (int line = 0; line < 5; line++) {
			
			for (int star = line+1; star < 5; star++) {
				System.out.print(" ");
			}
			for (int star = 0; star <= line; star++) {
				System.out.print("*");
			}
			System.out.println();
			
			}
		
		System.out.println("----------------------------------------");
		
			for (int line = 0; line < 5; line++) {
			
			for (int star = 1; star <= line; star++) {
				System.out.print(" ");
			}
			for (int star = 5; star > line; star--) {
				System.out.print("*");
			}
			System.out.println();
			
		}
		System.out.println("----------------------------------------");
		
	}

}


