			//int i = 20;
			System.out.println(i+","+a);
		}
		System.out.println("for 종료 ------------------------- ");
		
		int b = 0;
		for (; b < 3; b++) {
			System.out.println(b);
		}
		
		int c = 0;
		//for(int c; c < 3; c++)
		//for(c; c < 3; c++)
		for (c = 1; c < 3; c++) {

	}System.out.println("종료");

}
}

