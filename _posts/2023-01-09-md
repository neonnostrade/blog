{% highlight java %}

package day_05;

public class WhileMain {

	public static void main(String[] args) {
		
		int a = 10; //초기값
		while(a <= 20) {
			System.out.println("while 실행:"+a);
			a+=3; //증감
		}
		
		//while 문을 이용하여 0 -> 100 3의 배수 합을 구하세요
		
		int b = 0;
		int c = 0;
		while(b <= 100) {
			c += b;
			b += 3;			
		}
		System.out.println(c);
		
		
	}

}


