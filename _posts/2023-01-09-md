{% highlight java %}

package day_05;

public class DoWhileMain {

	public static void main(String[] args) {
		
		int a = 10;
		 while (a<5) {
			 System.out.println("while:"+a);
			 a++;
		 }	
		 System.out.println("while 종료---------------------------");
		 
		 a = 10;
		 do {
			 System.out.println("do~while:"+a);
			 a++;
		 }while(a<5);
		 System.out.println("do~while 종료---------------------------");
		 
	}

}


