{% highlight java %}

package day_02;

public class Exam {

	public static void main(String[] args) {
		
		double pos = 100; //0의 수만큼 소수점자릿수
		
		int cla = 2;
		System.out.println("반:"+cla);
		
		String name = "홍길동";
		System.out.println("성명:"+name);
		
		int eng = 89, math = 80, kor = 86;
		System.out.println("영어:"+eng);
		System.out.println("수학:"+math);
		System.out.println("국어:"+kor);
		
		int to = eng + math + kor;
		double avg = (double)to / 3;
		
		System.out.println("총점:"+to);
		System.out.println("평균:"+avg);
		
		/*
		 77.6666666667
		 	? * 100
		 7766.66666667+0.5
		 7767.16666667
		  7733.33333333+0.5 
		  7733.83333333
		 	? (int)
		 7767
		 	? (double)
		 7767.0
		 	? / 100
		 77.67
		 */
				
		double avg2 = (double)(int)(avg * pos+0.5)/pos; //100으로 곱한 직후 0.5를 더해 반올림
		
		System.out.println("평균2: "+avg2);
		
		String grade = avg2 >= 90 ? "수" :
					   avg2 >= 80 ? "우" :
					   avg2 >= 70 ? "미" :
					   avg2 >= 60 ? "양" : "가";
				
		System.out.println(avg2+":"+grade);
		
		// 90 > 80 > 70 > 60 순차적으로 해야 함
		
		if(avg >= 90) {
			System.out.println("수");
		}
		else if(avg >= 80) {
			System.out.println("우");
			if(eng >= 80 && math >= 80 && kor >= 80) {
				System.out.println("균등");
			}
				
		}
		else if(avg >= 70) {
			System.out.println("미");
		}
		else if(avg >= 60) {
			System.out.println("양");
		}
		else {
			System.out.println("가");
		}
		
		String grade2; //선생님
		
		if(avg >= 90) {
			grade2 = "수";
		}
		else if(avg >= 80) {
			grade2 = "우";
			if(eng >= 80 && math >= 80 && kor >= 80) {
				grade2 += "(균등)";
			}
				
		}
		else if(avg >= 70) {
			grade2 = "미";
		}
		else if(avg >= 60) {
			grade2 = "양";
		}
		else {
			grade2 = "가";
		}
		
		System.out.println("등급2: "+grade2);
		
		System.out.println((int)avg/10);
		
		String grade3;
		
		switch((int)avg/10) { //10을 나눠서 앞자리수로 분류
		case 10: // break가 없으면 다음꺼가 실행됨
		case 9:
			grade3 = "수";
			break;
		case 8:
			grade3 = "우";
			break;
		case 7:
			grade3 = "미";
			break;
		case 6:
			grade3 = "양";
			break;
		default:
			grade3 = "가";
			break;
		}
		
		System.out.println("등급3 : "+grade3);
		
		String grade4 = "";
		double avg3 = 3.8;
		
		switch((int)(avg3*2)) { // 10 / 5 = 2
		case 9:
			grade4 = "A+";
			break;
		case 8:
			grade4 = "A";
			break;
		case 7:
			grade4 = "B+";
			break;
		case 6:
			grade4 = "B";
			break;
		case 5:
			grade4 = "C+";
			break;
		case 4:
			grade4 = "C";
			break;
		case 3:
			grade4 = "D+";
			break;
		case 2:
			grade4 = "D";
			break;
		default:
			grade4 = "F";
		}
		
		System.out.println(avg3+" : "+grade4);
	}

}


