{% highlight java %}

package day_04;

public class SwitchMain {

	public static void main(String[] args) {
		
		switch(20) {
			case 70:
				System.out.println("70이지롱");
				break;
			case 50:
				System.out.println("50이지롱");
				break;
			default:
				System.out.println("기본값이지롱"); //switch의 else같은 존재 순서는 상관없음
				break;
			case 60:
				System.out.println("60이지롱");
				break;
			case 40:
				System.out.println("40이지롱");
				break;
			
		}
		
		System.out.println("switch 종료--------------------- 1");
		
		//정수, 문자, 문자열 가능
		//실수, boolean 불가
		//switch('a') {} o
		//switch("나는무너") {} o
		//switch('123.456') {} x
		//switch(true) {} x
		
		String grade = "과장";
		int basic = 300;
		double bonus = 0;
		
		switch(grade) {
		case "부장":
			bonus = 0.5;
			break;
		case "과장":
			bonus = 0.3;
			break;
		case "대리":
			bonus = 0.2;
			break;
		case "사원":
			bonus = 0.1;
			break;
	}
		int money = basic + (int)(basic * bonus); //bonus가 double이라 형변환 해야함
		
		System.out.println(money);
		
		System.out.println("switch 종료--------------------- 2");
		
		String coffee = "아프리카노";
		int cos = 0;
		int num = 3;
				
		switch(coffee) {
		case "아메리카노":
			cos = 2000;
			break;
		case "아시아노":
			cos = 3000;
			break;
		case "아프리카노":
			cos = 4000;
			break;	
	}
		int order = cos * num;
		
		/*System.out.println("커피종류 : "+coffee);
		System.out.println("잔수 : "+num);*/
		System.out.println(coffee+" "+num+"잔"+" : "+order+"원");

	}
}


